{
  "name": "igorsexy",
  "displayName": "igorsexy",
  "description": "igor best lover",
  "version": "1.0.2",
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.98.0"
  },
  "menus": {
    "editor/context": [
      {
        "command": "extension.igorOnHelp",
        "when": "editorTextFocus",
        "group": "navigation"
      }
    ]
  },
  "publisher": "webhuntru",
  "repository": {
    "type": "git",
    "url": "https://github.com/igorushakov05/extension.git"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:igorsexy.helloWorld"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "igorsexy.helloWorld",
        "title": "Igorsexy"
      },
      {
        "command": "extension.igorOnHelp",
        "title": "Игорь на помощь"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.98.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.21.0"
  },
  "dependencies": {
    "axios": "^1.8.4"
  }
}
